<h1>Users</h1>


<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Full Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user|%>
      <tr>
        <td><a href="/users/<%= user.user_id %>"><%= user.first_name %> <%= user.last_name %></a></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<button type="button" name="button"><a href="users/new">New entry</a></button>
