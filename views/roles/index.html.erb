<h1>Roles</h1>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Role Name</th>
    </tr>
  </thead>
  <tbody>
    <% @roles.each do |role|%>
      <tr>
        <td><a href="/roles/<%= role.role_id %>"></a><%= role.role_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<button type="button" name="button"><a href="roles/new">New Entry</a></button>
